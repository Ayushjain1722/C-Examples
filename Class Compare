#include<iostream>
#include<cstring>
using namespace std;
class person
{	char name[20];
	int age;
    public:
	void setData(const char *s, int a)








	person  greater(person &x)
	{	if(x.age >= age)	return x;
		else	return *this;	
	}
	void display()
	{	cout << name << " with age " << age;	}
};
int main()
{	person p1,p2,p3;
	p1.set1);

	p2.setData("Varun", 19);
	p3.setData("Shobhit",19);

	p2.setData("Akhil", 29);






	person p = p1.greater(p2);
	cout << "Elder person is: ";
	p.display();
	p=p2.greater(p3);
	cout << endl << "Elder person is: ";
	p.display();
	return 0;
}
